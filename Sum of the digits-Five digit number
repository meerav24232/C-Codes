#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
	
    int n;
    scanf("%d", &n);
    int rem=n%10;
    n=n/10;
    int rem2=n%10;
    n=n/10;
    int rem3=n%10;
    n=n/10;
    int rem4=n%10;
    n=n/10;
    int rem5=n%10;
    n=n/10;
    int sum=rem+rem2+rem3+rem4+rem5;
    printf ("%d",sum);
    return 0;
}
